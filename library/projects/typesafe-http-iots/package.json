{
  "name": "@peppierre/typesafe-http-iots",
  "version": "1.0.0",
  "author": "PePPierre <peppierre@gmail.com>",
  "description": "Type-safe HTTP client for Angular using io-ts and fp-ts",
  "keywords": [
    "angular",
    "http",
    "iots",
    "fp-ts",
    "typescript"
  ],
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@angular/common": "~16.0.0",
    "@angular/core": "~16.0.0",
    "fp-ts": "^2.16.10",
    "io-ts": "^2.2.22",
    "rxjs": "~7.8.0"
  },
  "sideEffects": false,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "modulePaths": [
      "<rootDir>/projects/typesafe-http-iots/src/"
    ],
    "moduleNameMapper": {
      "typesafe-http-iots": "<rootDir>/projects/typesafe-http-iots/src/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "projects/typesafe-http-iots/src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "projects/typesafe-http-iots"
    ],
    "testRegex": ".spec.ts$"
  }
}
